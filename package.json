{
  "name": "electron-vite-template",
  "version": "1.0.0",
  "description": "Really simple Electron28 + Vue3 + Vite5 template.",
  "author": "sxgan <sxgan@foxmail.com>",
  "private": true,
  "main": "dist-electron/main/index.js",
  "license": "MIT",
  "keywords": [
    "electron",
    "rollup",
    "vite",
    "vue3",
    "vue"
  ],
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:9999/"
    }
  },
  "type": "module",
  "scripts": {
    "dev": "chcp 65001 && vite --mode dev",
    "build": "chcp 65001 && vue-tsc --noEmit && vite build --mode pro && electron-builder",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.1",
    "electron": "^28.1.0",
    "electron-builder": "^24.9.1",
    "tree-kill": "^1.2.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vite-plugin-electron": "^0.28.0",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vue": "^3.4.1",
    "vue-tsc": "^1.8.27",
    "@iconify-json/ep": "^1.1.14",
    "@iconify/vue": "^4.1.1",
    "unplugin-auto-import": "^0.17.4",
    "unplugin-icons": "^0.18.2",
    "unplugin-vue-components": "^0.26.0",
    "vite-plugin-svg-icons": "^2.0.1"
  },
  "dependencies": {
    "vue-router": "^4.2.5",
    "@element-plus/icons-vue": "^2.3.1",
    "@highlightjs/vue-plugin": "^2.1.0",
    "@iconify-json/ep": "^1.1.14",
    "@types/mockjs": "^1.0.10",
    "axios": "^1.6.7",
    "element-plus": "^2.5.3",
    "highlight.js": "^11.9.0",
    "mockjs": "^1.1.0",
    "pinia": "^2.1.7",
    "sass": "^1.70.0",
    "vue": "^3.3.4"
  }
}
