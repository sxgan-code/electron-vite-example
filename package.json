{
  "name": "electron-vite-template",
  "version": "1.0.0",
  "description": "Really simple Electron28 + Vue3 + Vite5 template.",
  "author": "sxgan <sxgan@foxmail.com>",
  "private": true,
  "main": "dist-electron/main/index.js",
  "license": "MIT",
  "keywords": [
    "electron",
    "rollup",
    "vite",
    "vue3",
    "vue"
  ],
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:9999/"
    }
  },
  "type": "module",
  "scripts": {
    "dev": "chcp 65001 && vite",
    "build": "chcp 65001 && vue-tsc --noEmit && vite build && electron-builder",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.1",
    "electron": "^28.1.0",
    "electron-builder": "^24.9.1",
    "tree-kill": "^1.2.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vite-plugin-electron": "^0.28.0",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vue": "^3.4.1",
    "vue-tsc": "^1.8.27"
  },
  "dependencies": {
    "vue-router": "^4.2.5"
  }
}
